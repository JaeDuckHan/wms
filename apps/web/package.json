{
  "name": "wms-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "typecheck": "tsc --noEmit --pretty false --incremental false",
    "i18n:check": "node scripts/check_i18n_keys.mjs",
    "test:snapshot:detail-i18n": "node scripts/test_detail_i18n_snapshot.mjs",
    "test:snapshot:detail-i18n:update": "node scripts/test_detail_i18n_snapshot.mjs --update",
    "web:check": "npm run typecheck && npm run i18n:check && npm run test:snapshot:detail-i18n && npm run build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.475.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
